{
    "name": "adeptoas/fancy",
    "description": "Fancy Functions Collection",
    "type": "library",
    "version": "1.3.1",
    "license": "MIT",
    "authors": [
        {
            "name": "FeistyBall",
            "email": "cg@adepto.as"
        },
    
        {
            "name": "bluefirex",
            "email": "bs@adepto.as"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "danielstjules/stringy": "^2.4",
        "php": ">=7.4",
        "ext-mbstring": "*",
		"ext-imagick": "*",
        "ext-iconv": "*"
    },
    "autoload": {
        "psr-4": {
            "Adepto\\Fancy\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "suggest": {
        "ext-xml": "Required for some XML functionality",
        "ext-xmlwriter": "Required for some XML functionality"
    },
    "scripts": {
        "test": "php vendor/bin/phpunit -c phpunit.xml --colors=always"
    }
}
